{
	"extends": [
		"@remix-run/eslint-config",
		"@remix-run/eslint-config/node"
	],
	"rules": {
		// no unused vars
		"@typescript-eslint/no-unused-vars": [
			"warn",
			{
				"vars": "all",
				"args": "after-used",
				"ignoreRestSiblings": false,
				"argsIgnorePattern": "^_",
				"varsIgnorePattern": "^_"
			}
		]
	}
}